<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/DistribuidorTestImpresoraLinearLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    tools:context="${relativePackage}.${activityClass}">

    <ImageView
        android:id="@+id/imgvLoginLogotipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bullet_eliminar" />

    <TextView
        android:id="@+id/tvVendedorVentaDirectaProductoCliente"
        style="@style/Etiqueta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:width="125dp"
        android:maxWidth="125dp"
        android:minWidth="125dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tvPreventaDisenioCantidad"
        style="@style/Etiqueta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:width="20dp"
        android:gravity="center_horizontal"
        android:maxWidth="20dp"
        android:minWidth="20dp"
        android:text="00" />

    <TextView
        android:id="@+id/tvPreventaDisenioPrecioUnitario"
        style="@style/Etiqueta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:width="40dp"
        android:gravity="right"
        android:maxWidth="40dp"
        android:minWidth="40dp"
        android:text="000.00" />

    <TextView
        android:id="@+id/tvVendedorPreventaProductoProveedor"
        style="@style/Etiqueta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:width="40dp"
        android:gravity="right"
        android:maxWidth="40dp"
        android:minWidth="40dp"
        android:text="000.00" />

    <TextView
        android:id="@+id/tvVendedorPreventaProductoProducto"
        style="@style/Etiqueta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:width="60dp"
        android:gravity="right"
        android:maxWidth="60dp"
        android:minWidth="60dp"
        android:text="0000.00" />

</LinearLayout>
