<?xml version="1.0" encoding="utf-8"?>
<fragment xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:map="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/mapaVendedor"
	android:name="com.google.android.gms.maps.SupportMapFragment"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<CheckBox
		android:id="@+id/cbVendedorMapaClienteCenso"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:checked="false"
		android:clickable="false"
		android:layout_gravity="top"
		android:text="Censo" />

	<CheckBox
		android:id="@+id/cbVendedorMapaClienteVender"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:checked="false"
		android:clickable="false"
		android:layout_gravity="top|center_horizontal"
		android:text="Vender" />

	<RadioButton
		android:id="@+id/rbtnVendedorMapaClienteMatch"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:checked="false"
		android:clickable="true"
		android:layout_gravity="top|right"
		android:text="Match"/>

	<CheckBox
		android:id="@+id/cbVendedorMapaEncuesta"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:checked="false"
		android:clickable="true"
		android:layout_gravity="bottom|center_horizontal"
		android:text="Encuesta" />
</fragment>


    
