<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/DistribuidorTestImpresoraLinearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="${relativePackage}.${activityClass}" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tvLogAplicacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="30dp"
            android:text="tvLogAplicacion"
            android:textAppearance="@style/Contenido"
            android:width="30dp" />

        <TextView
            android:id="@+id/tvLogFecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:maxWidth="80dp"
            android:text="tvLogFecha"
            android:textAppearance="@style/Etiqueta"
            android:width="80dp" />

        <TextView
            android:id="@+id/tvLogActividad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:maxWidth="200dp"
            android:text="tvLogActividad"
            android:textAppearance="@style/Contenido"
            android:width="200dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tvLogTipoLog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="50dp"
            android:text="tvLogTipoLog"
            android:textAppearance="@style/EtiquetaRequerida"
            android:width="50dp" />

        <TextView
            android:id="@+id/tvVendedorVentaDirectaPromocionProveedor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="270dp"
            android:text="TextView"
            android:textAppearance="@style/Etiqueta"
            android:width="270dp" />

    </LinearLayout>

</LinearLayout>
