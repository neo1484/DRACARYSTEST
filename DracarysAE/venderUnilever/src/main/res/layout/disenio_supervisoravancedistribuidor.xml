<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/llCensistaEdicionCliente"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="${relativePackage}.${activityClass}" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tvDisenioAvanceDistribuidorDistribuidor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="320dp"
            android:minWidth="320dp"
            android:text="Distribuidor"
            android:textAppearance="@style/Etiqueta"
            android:width="320dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tvDisenioAvanceDistribuidorNoPreventas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:maxWidth="35dp"
            android:minWidth="35dp"
            android:text="00000"
            android:textAppearance="@style/Etiqueta"
            android:width="35dp" />

        <TextView
            android:id="@+id/tvDisenioAvanceDistribuidorMontoPreventas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:maxWidth="65dp"
            android:minWidth="65dp"
            android:text="000000.00"
            android:textAppearance="@style/Etiqueta"
            android:width="65dp" />

        <TextView
            android:id="@+id/tvDisenioAvanceDistribuidorNoVentas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:maxWidth="35dp"
            android:minWidth="35dp"
            android:text="00000"
            android:textAppearance="@style/Contenido"
            android:width="35dp" />

        <TextView
            android:id="@+id/tvDisenioAvanceDistribuidorMontoVentas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:maxWidth="65dp"
            android:minWidth="65dp"
            android:text="000000.00"
            android:textAppearance="@style/Contenido"
            android:width="65dp" />

        <TextView
            android:id="@+id/tvDisenioAvanceDistribuidorEficiencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:maxWidth="40dp"
            android:minWidth="40dp"
            android:text="0000"
            android:textAppearance="@style/Etiqueta"
            android:width="40dp" />

    </LinearLayout>

</LinearLayout>
